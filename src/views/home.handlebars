<div>
    <h1>La lista de productos es: </h1>
    {{#each arrayProd}}
    <ul>
        <li>{{this.title}}</li>
        <li>{{this.description}}</li>
        <li>{{this.price}}</li>
        <li>{{this.thumbnail}}</li>
        <li>{{this.code}}</li>
        <li>{{this.stock}}</li>
        <li>{{this.category}}</li>
        <li>{{this.picture}}</li>
        <li>{{this.id}}</li>
    </ul>
    {{/each}}
</div>